    children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: `max-w-sm w-full ${styles.bgColor} ${styles.borderColor} border rounded-lg shadow-lg`,
      children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: "p-4",
        children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "flex items-start",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "flex-shrink-0",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)(Icon, {
              className: `h-5 w-5 ${styles.iconColor}`
            })
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "ml-3 w-0 flex-1",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)("p", {
              className: `text-sm font-medium ${styles.titleColor}`,
              children: title
            }),
            /* istanbul ignore next */
            (cov_peh9q106m().b[22][0]++, description) &&
            /* istanbul ignore next */
            (cov_peh9q106m().b[22][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
              className: `mt-1 text-sm ${styles.textColor}`,
              children: description
            }))]
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "ml-4 flex-shrink-0 flex",
            children: /*#__PURE__*/(0, _jsxruntime.jsxs)("button", {
              onClick: onClose,
              className: `rounded-md inline-flex ${styles.textColor} hover:${styles.titleColor} focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-offset-green-50 focus:ring-green-600`,
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                className: "sr-only",
                children: "Close"
              }), /*#__PURE__*/(0, _jsxruntime.jsx)(_outline.XMarkIcon, {
                className: "h-4 w-4"
              })]
            })
          })]
        })
      })
    })
  });
};
