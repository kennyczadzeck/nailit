{
  "name": "nailit",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:fast": "jest tests/unit tests/fixtures --passWithNoTests",
    "test:integration": "jest tests/integration tests/features/api --passWithNoTests",
    "test:bdd": "jest tests/bdd tests/features --passWithNoTests",
    "test:features": "jest tests/features --passWithNoTests",
    "test:e2e": "jest tests/e2e --passWithNoTests",
    "test:ci:fast": "jest tests/unit tests/fixtures --ci --coverage=false --passWithNoTests",
    "test:ci:integration": "jest tests/integration tests/features/api --ci --coverage=false --passWithNoTests",
    "test:ci:all": "jest --ci --coverage --passWithNoTests",
    "test:ci:smoke": "jest tests/e2e/smoke --ci --coverage=false --passWithNoTests",
    "test:feature:auth": "jest tests/features/authentication --passWithNoTests",
    "test:feature:projects": "jest tests/features/projects --passWithNoTests",
    "test:feature:api": "jest tests/features/api --passWithNoTests",
    "test:feature:components": "jest tests/features/components --passWithNoTests",
    "lint:check": "next lint --max-warnings 0",
    "lint:ci": "next lint --max-warnings 20",
    "type-check": "tsc --noEmit",
    "build:test": "next build && npm run test:ci:all",
    "deploy:staging": "npm run test:ci:all && npm run build",
    "deploy:prod": "npm run test:ci:all && npm run build",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed",
    "db:studio": "npx prisma studio",
    "test:oauth-setup": "tsx scripts/email-testing/oauth-setup.ts setup",
    "test:oauth-callback": "tsx scripts/email-testing/oauth-setup.ts callback",
    "test:send-email": "tsx scripts/email-testing/email-sender.ts single",
    "test:send-bulk-emails": "tsx scripts/email-testing/email-sender.ts bulk",
    "test:truncate-all": "tsx scripts/email-testing/data-manager.ts truncate-all",
    "test:truncate-db": "tsx scripts/email-testing/data-manager.ts truncate-db",
    "test:truncate-s3": "tsx scripts/email-testing/data-manager.ts truncate-s3",
    "test:cleanup": "tsx scripts/email-testing/data-manager.ts cleanup",
    "test:reset-account": "tsx scripts/email-testing/data-manager.ts reset-account",
    "test:query-emails": "tsx scripts/email-testing/data-manager.ts query-emails",
    "test:check-s3": "tsx scripts/email-testing/data-manager.ts check-s3",
    "test:setup-webhook": "tsx scripts/email-testing/webhook-tester.ts setup",
    "test:stop-webhook": "tsx scripts/email-testing/webhook-tester.ts stop",
    "test:validate-webhook": "tsx scripts/email-testing/webhook-tester.ts test",
    "test:check-webhook-subscription": "tsx scripts/email-testing/webhook-tester.ts validate",
    "test:email-complete": "tsx scripts/email-testing/master-test.ts complete",
    "test:email-smoke": "tsx scripts/email-testing/master-test.ts smoke",
    "test:email-setup": "tsx scripts/email-testing/master-test.ts setup",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@aws-sdk/client-cloudwatch-logs": "^3.821.0",
    "@aws-sdk/client-s3": "^3.840.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.8.2",
    "@supabase/supabase-js": "^2.39.0",
    "@types/pg": "^8.15.2",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "googleapis": "^150.0.1",
    "next": "15.4.0-canary.51",
    "next-auth": "^4.24.11",
    "pg": "^8.16.0",
    "prisma": "^6.8.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^2.2.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "eslint": "^9",
    "eslint-config-next": "15.4.0-canary.51",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "msw": "^2.8.7",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5"
  }
}
